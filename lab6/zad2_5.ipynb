    "\n",
    "param_combinations = list(product(population_sizes, mutation_rates, crossover_rates))\n",
    "\n",
    "def run_experiment(params):\n",
    "    pop_size, mut_rate, cross_rate = params\n",
    "    start_time = time.time()\n",
    "\n",
    "    results = []\n",
    "    for _ in range(5):  # Repeat 5 times\n",
